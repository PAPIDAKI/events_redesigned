<article id="'user">
<header>
  <h1><%=@user.name%></h1>
  <h2><%=@user.email%></h2>

</header>

<footer>
  <nav>
    <%=link_to "Edit Acount",edit_user_path(@user),class:'button'%>
    <%=link_to "Delete Acount",@user,method: :delete,
                data: {confirm: "Are you sure?"},
                class:'button'%>

  </nav>
</footer>
</article>